<% if (title) { %>
    <h1><%= title %></h1>
<% } else {
    res.redirect('/')
} %>

<%- messages() %>

<table>
  <thead>
    <tr>
      <th>User ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Account Type</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
      <tr>
        <td><%= user.account_id %></td>
        <td><%= user.account_firstname %></td>
        <td><%= user.account_lastname %></td>
        <td><%= user.account_email %></td>
        <td><%= user.account_type %></td>
        <td>
          <a href="/admin/users/<%= user.account_id %>">View</a>
          <a href="/admin/users/edit/<%= user.account_id %>">Edit</a>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>